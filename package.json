{
    "name": "useful-useless",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "start": "rimraf .cache dist && parcel src/index.html --open",
        "build": "rimraf dist && parcel build src/index.html --no-cache --no-source-maps --detailed-report",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "src/**/*.jsx": [
            "prettier --write",
            "git add"
        ],
        "src/**/*.scss": [
            "stylelint --syntax scss",
            "git add"
        ]
    },
    "dependencies": {
        "react": "^16.6.0",
        "react-dom": "^16.6.0"
    },
    "devDependencies": {
        "eslint": "^5.8.0",
        "csslint": "^1.0.5",
        "babel-eslint": "^8.2.6",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-prettier": "^2.6.2",
        "eslint-plugin-react": "^7.10.0",
        "parcel": "^1.9.4",
        "prettier-eslint": "^8.8.2",
        "stylelint": "^9.3.0",
        "stylelint-config-recommended": "^2.1.0",
        "rimraf": "^2.6.2"
    },
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    }
}
